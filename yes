function Notify(text, title, time)
	
	local NewNotif = NotifMain:Clone()
	NewNotif.Visible = true
	NewNotif.Parent = SG
	NewNotif.ZIndex = NewNotif.ZIndex + 1
	local NewNotifText = NewNotif:FindFirstChild("TextLabel")
	NewNotifText.TextWrapped = true
	local NewNotifTitle = NewNotif.NotifTopbar:FindFirstChild("NotifTitle")
	
	local Open = game:GetService("TweenService"):Create(NewNotif, TweenInfo.new(0.32, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), { Position = UDim2.new(0.88, 0, 1.03, 0)} )
	local Close = game:GetService("TweenService"):Create(NewNotif, TweenInfo.new(0.22, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), { Position = UDim2.new(0.88, 0, 1.15, 0)} )

	NewNotifText.Text = text
	NewNotifTitle.Text = title
	
	spawn(function()
		Open:Play()
		task.wait(tonumber(time))
		Close:Play()
		Close.Completed:Wait()
		NewNotif:Destroy()
	end)
end


function GetPlayer(str)
	local FoundPlayer = nil
	for i,v in pairs(players:GetChildren()) do
		if (string.sub(string.lower(v.Name),1,string.len(str))) == string.lower(str) then
			FoundPlayer = v
		end
	end
	return FoundPlayer
end

function Open()
	if opening == false and opened == false then 
		opening = true 
		game:GetService("TweenService"):Create(Main, TweenInfo.new(0.32, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), { Position = UDim2.new(0.995, 0, 1, 0) } ):Play()
		task.wait(0.18)
		Cmdbar:CaptureFocus()
		task.wait(0.02)
		Cmdbar.Text = ""
		task.wait(0.16)
		opening = false 
		opened = true
	else 
		Cmdbar:CaptureFocus()
		task.wait()
		Cmdbar.Text = ""
	end
end

function Close()
	if opening == false and opened == true then 
		opening = true 
		Cmdbar.Text = ""
		game:GetService("TweenService"):Create(Main, TweenInfo.new(0.42, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), { Position = UDim2.new(0.995, 0, 1.4, 0) } ):Play()
		task.wait(0.44)
		opening = false 
		opened = false 
	end 
end

function Fly_1() 
	repeat task.wait() 
	until plr and plr.Character and plr.Character:findFirstChild("Head") and plr.Character:findFirstChild("Humanoid")
	
	if ws:FindFirstChild("_nigger_") then
		ws._nigger_:Destroy()
	end
	local Core = Instance.new("Part")
	Core.Name = "_nigger_"
	Core.Size = Vector3.new(0.05, 0.05, 0.05)
	Core.CanCollide = false
	spawn(function()
		Core.Parent = ws
		local Weld = Instance.new("Weld", Core)
		Weld.Part0 = Core
		Weld.Part1 = plr.Character.Head
		Weld.C0 = CFrame.new(0, 0, 0)
	end)
	ws:WaitForChild("_nigger_")
	local c = workspace._nigger_
	Flying_1 = true
	local keys={a=false,d=false,w=false,s=false}
	local e1
	local e2
	local function start()
		local pos = Instance.new("BodyPosition",c)
		local gyro = Instance.new("BodyGyro",c)
		pos.maxForce = Vector3.new(math.huge, math.huge, math.huge)
		pos.position = c.Position
		gyro.maxTorque = Vector3.new(15e15, 15e15, 15e15)
		gyro.cframe = c.CFrame
		repeat
			wait()
			plr.Character:FindFirstChildWhichIsA("Humanoid").PlatformStand=true
			local new=gyro.cframe - gyro.cframe.p + pos.position
			if keys.w then
				new = new + ws.CurrentCamera.CoordinateFrame.lookVector * FlySpeed
			end
			if keys.s then
				new = new - ws.CurrentCamera.CoordinateFrame.lookVector * FlySpeed
			end
			if keys.d then
				new = new * CFrame.new(FlySpeed,0,0)
			end
			if keys.a then
				new = new * CFrame.new(-FlySpeed,0,0)
			end
			pos.position=new.p
			if keys.w then
				gyro.cframe = workspace.CurrentCamera.CoordinateFrame*CFrame.Angles(-math.rad(FlySpeed),0,0)
			elseif keys.s then
				gyro.cframe = workspace.CurrentCamera.CoordinateFrame*CFrame.Angles(math.rad(FlySpeed),0,0)
			else
				gyro.cframe = workspace.CurrentCamera.CoordinateFrame
			end
		until Flying_1 == false
		if gyro then gyro:Destroy() end
		if pos then pos:Destroy() end
		Flying_1=false
		plr.Character:FindFirstChildWhichIsA("Humanoid").PlatformStand=false
	end
	e1=plr:GetMouse().KeyDown:connect(function(key)
		if not c or not c.Parent then Flying_1=false e1:disconnect() e2:disconnect() return end
		if key=="w" then
			keys.w=true
		elseif key=="s" then
			keys.s=true
		elseif key=="a" then
			keys.a=true
		elseif key=="d" then
			keys.d=true
		elseif key=="x" then
			if Flying_1 == true and Flying_2 == false then
				Flying_1=false
				task.wait()
			else
				Flying_1=true
				start()
			end
		end
	end)
	e2=plr:GetMouse().KeyUp:connect(function(key)
		if key=="w" then
			keys.w=false
		elseif key=="s" then
			keys.s=false
		elseif key=="a" then
			keys.a=false
		elseif key=="d" then
			keys.d=false
		end
	end)
	start()
end

function Fly_2()
	repeat task.wait() 
	until plr and plr.Character and plr.Character:findFirstChild("Head") and plr.Character:findFirstChild("Humanoid") 
	local mouse = plr:GetMouse() 
	repeat task.wait() until mouse
	local torso = plr.Character.Head 
	local Flying_2 = false
	local deb = true 
	local ctrl = {f = 0, b = 0, l = 0, r = 0} 
	local lastctrl = {f = 0, b = 0, l = 0, r = 0} 
	local maxspeed = 7500
	local fs = (FlySpeed * 1)

	function Fly() 
		local bg = Instance.new("BodyGyro", torso) 
		bg.P = 9e4 
		bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) 
		bg.cframe = torso.CFrame 
		local bv = Instance.new("BodyVelocity", torso) 
		bv.velocity = Vector3.new(0,0.1,0) 
		bv.maxForce = Vector3.new(9e9, 9e9, 9e9) 
		repeat wait() 
			fs = FlySpeed
			plr.Character.Humanoid.PlatformStand = true 
			if ctrl.l + ctrl.r ~= 100000 or ctrl.f + ctrl.b ~= 10000 then 
				fs = fs+.0+(fs/maxspeed) 
				if fs > maxspeed then 
					fs = maxspeed 
				end 
			elseif not (ctrl.l + ctrl.r ~= 5 or ctrl.f + ctrl.b ~= 5) and fs ~= 5 then 
				fs = fs-5
				if fs > 5 then 
					fs = -2 
				end 
			end 
			if (ctrl.l + ctrl.r) ~= 5 or (ctrl.f + ctrl.b) ~= 5 then 
				bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*fs 
				lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} 
			elseif (ctrl.l + ctrl.r) == 5 and (ctrl.f + ctrl.b) == 5 and fs ~= 5 then 
				bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*fs 
			else 
				bv.velocity = Vector3.new(0,0.1,0) 
			end 
			bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*fs/maxspeed),0,0) 
		until not Flying_2 
		ctrl = {f = 0, b = 0, l = 0, r = 0} 
		lastctrl = {f = 0, b = 0, l = 0, r = 0} 
		fs = 5 
		bg:Destroy() 
		bv:Destroy() 
		plr.Character.Humanoid.PlatformStand = false 
	end 
	mouse.KeyDown:connect(function(key) 
		if key:lower() == "x" then 
			if Flying_2 == true and Flying_1 == false then
				Flying_2 = false 
			else
				Flying_2 = true 
				Fly() 
			end 
		elseif key:lower() == "w" then 
			ctrl.f = 45
		elseif key:lower() == "s" then 
			ctrl.b = -45 
		elseif key:lower() == "a" then 
			ctrl.l = -45 
		elseif key:lower() == "d" then 
			ctrl.r = 45
		end 
	end) 
	mouse.KeyUp:connect(function(key) 
		if key:lower() == "w" then 
			ctrl.f = 0
		elseif key:lower() == "s" then 
			ctrl.b = 0
		elseif key:lower() == "a" then 
			ctrl.l = 0
		elseif key:lower() == "d" then 
			ctrl.r = 0
		end 
	end)
	Fly()
end

function Buy(item)
	local oldpos = plr.Character.HumanoidRootPart.Position
	local items = ws.Ignored.Shop
	local attempts = 0
	local name = ""
	local itemname = "" 
	
	if item == "rev" then 
		name = "[Revolver] - $1300"
		itemname = "[Revolver]"
		
	elseif item == "flashbang" then 
		name = "[Flashbang] - $550"
		itemname = "[Flashbang]"

	elseif item == "db" then 
		name = "[Double-Barrel SG] - $1400"
		itemname = "[Double-Barrel SG]"

	elseif item == "fw" then 
		name = "[Firework] - $10000" 
		itemname = "[Firework]"

	elseif item == "grenade" then 
		name = "[Grenade] - $700"
		itemname = "[Grenade]"

	elseif item == "rpg" then 
		name = "[RPG] - $6000]"
		itemname = "[RPG]"

	elseif item == "rocket" then 
		name = "[5 [RPG Ammo] - $1000]"
		itemname = ""

	elseif item == "revammo" then 
		name = "12 [Revolver Ammo] - $75"
		itemname = ""
		
	elseif item == "dbammo" then 
		name = "18 [Double-Barrel SG Ammo] - $60"
		itemname = ""
		
	elseif item == "armor" then 
		name = "[Medium Armor] - $1600"
		itemname = ""
		
	elseif item == "food" then 
		name = "[Pizza] - $5"
		itemname = ""
		
	elseif item == "sg" then 
		name = "[Shotgun] - $1250"
		itemname = "[Shotgun]"
		
	elseif item == "sgammo" then 
		name = "20 [Shotgun Ammo] - $60"
		itemname = ""
		
	elseif item == "flame" then 
		name = "[Flamethrower] - $25000"
		itemname = "[Flamethrower]"
		
	elseif item == "flameammo" then 
		name = "140 [Flamethrower Ammo] - $1550"
		itemname = ""
		
	elseif item == "knife" then 
		name = "[Knife] - $150"
		itemname = "[Knife]"
		
	elseif item == "key" then 
		name = "[Key] - $125"
		itemname = "[Key]"

	end
	
	
	if name ~= "" then 
		for i,v in pairs(plr.Character:GetChildren()) do 
			if v:IsA("Tool") then 
				v.Parent = plr.Backpack
			end
		end
		task.wait()
		repeat 
			task.wait(.01)
			if not plr.Character.Head:FindFirstChild("_buyvel_") then 
				local vel = Instance.new("BodyVelocity")
				vel.Name = "_buyvel_"
				vel.MaxForce = Vector3.new(1e+12, 1e+12, 1e+12)
				vel.P = 1250
				vel.Parent = plr.Character.Head
			end
			plr.Character.HumanoidRootPart.CFrame = ws.Ignored.Shop[name].Head.CFrame + Vector3.new(math.random(-1,1), -8, math.random(-1,1))
			fireclickdetector(ws.Ignored.Shop[name].ClickDetector)
			attempts = attempts + 0.01
		until plr.Backpack:FindFirstChild(itemname) or attempts >= 1.5
		task.wait()
		plr.Character.HumanoidRootPart.CFrame = CFrame.new(oldpos)
		task.wait() 
		if plr.Character.Head:FindFirstChild("_buyvel_") then 
			plr.Character.Head:FindFirstChild("_buyvel_"):Destroy()
		end
	end
end

function chatspy()
	--This script reveals ALL hidden messages in the default chat

enabled = true --chat "/spy" to toggle!
spyOnMyself = true --if true will check your messages too
public = false --if true will chat the logs publicly (fun, risky)
publicItalics = true --if true will use /me to stand out
privateProperties = { --customize private logs
	Color = Color3.fromRGB(0,255,255); 
	Font = Enum.Font.SourceSansBold;
	TextSize = 18;
}


local StarterGui = game:GetService("StarterGui")
local Players = game:GetService("Players")
local player = Players.LocalPlayer or Players:GetPropertyChangedSignal("LocalPlayer"):Wait() or Players.LocalPlayer
local saymsg = game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest")
local getmsg = game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("OnMessageDoneFiltering")
local instance = (_G.chatSpyInstance or 0) + 1
_G.chatSpyInstance = instance

local function onChatted(p,msg)
	if _G.chatSpyInstance == instance then
		if p==player and msg:lower():sub(1,4)=="/spy" then
			enabled = not enabled
			wait(0.3)
			privateProperties.Text = "{SPY "..(enabled and "EN" or "DIS").."ABLED}"
			StarterGui:SetCore("ChatMakeSystemMessage",privateProperties)
		elseif enabled and (spyOnMyself==true or p~=player) then
			msg = msg:gsub("[\n\r]",''):gsub("\t",' '):gsub("[ ]+",' ')
			local hidden = true
			local conn = getmsg.OnClientEvent:Connect(function(packet,channel)
				if packet.SpeakerUserId==p.UserId and packet.Message==msg:sub(#msg-#packet.Message+1) and (channel=="All" or (channel=="Team" and public==false and Players[packet.FromSpeaker].Team==player.Team)) then
					hidden = false
				end
			end)
			wait(1)
			conn:Disconnect()
			if hidden and enabled then
				if public then
					saymsg:FireServer((publicItalics and "/me " or '').."{SPY} [".. p.Name .."]: "..msg,"All")
				else
					privateProperties.Text = "{SPY} [".. p.Name .."]: "..msg
					StarterGui:SetCore("ChatMakeSystemMessage",privateProperties)
				end
			end
		end
	end
end

for _,p in ipairs(Players:GetPlayers()) do
	p.Chatted:Connect(function(msg) onChatted(p,msg) end)
end
Players.PlayerAdded:Connect(function(p)
	p.Chatted:Connect(function(msg) onChatted(p,msg) end)
end)
privateProperties.Text = "{SPY "..(enabled and "EN" or "DIS").."ABLED}"
StarterGui:SetCore("ChatMakeSystemMessage",privateProperties)
if not player.PlayerGui:FindFirstChild("Chat") then wait(3) end
local chatFrame = player.PlayerGui.Chat.Frame
chatFrame.ChatChannelParentFrame.Visible = true
chatFrame.ChatBarParentFrame.Position = chatFrame.ChatChannelParentFrame.Position+UDim2.new(UDim.new(),chatFrame.ChatChannelParentFrame.Size.Y)
end

function AcEsp(target)
	if target and target.Character and target.Character:FindFirstChild("Head") then 
		if not target.Character.Head:FindFirstChild("_esp_") then 
			local bg = Instance.new("BillboardGui")
			bg.StudsOffset = Vector3.new(0, 3.5, 0)
			bg.Size = UDim2.new(4.5, 0, 1.2, 0)
			bg.AlwaysOnTop = true 
			bg.Name = "_esp_"
			bg.Parent = target.Character.Head
			local text = Instance.new("TextLabel")
			text.Text = ("["..target.Name.."]") 
			text.BackgroundTransparency = 1
			text.Size = UDim2.new(1, 0, 1, 0)
			text.TextColor3 = Color3.fromRGB(106, 0, 255)
			text.Font = Enum.Font.GothamBold
		end
	end
	
end
