	elseif c1 == "copyid" then 
		local t = GetPlayer(c2)
		task.wait()
		setclipboard(tostring(t.UserId))
		
	elseif c1 == "copyuser" then 
		for i,v in pairs(players:GetChildren()) do
			if (string.sub(string.lower(v.DisplayName),1,string.len(c2))) == string.lower(c2) then
				setclipboard(v.Name)
			end
		end
		
	elseif c1 == "rj" then 
		game:GetService("TeleportService"):Teleport(game.PlaceId, plr)
		
	elseif c1 == "shop" then 
		local servers = {}
		for _, v in ipairs(game:GetService("HttpService"):JSONDecode(game:HttpGetAsync("https://games.roblox.com/v1/games/" .. game.PlaceId .. "/servers/Public?sortOrder=Asc&limit=100")).data) do
			if type(v) == "table" and v.maxPlayers > v.playing and v.id ~= game.JobId then
				servers[#servers + 1] = v.id
			end
		end
		if #servers > 0 then
			game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, servers[math.random(1, #servers)])
		else
			return Notify("No server found.", "SERVER HOP", 5)
		end
		
	elseif c1 == "knock" then 
		KnockOldPos = plr.Character.HumanoidRootPart.Position
		Knock = GetPlayer(c2)
		Targeting = nil
		TargetNoclipping = true 
		TargetReach = true 
		
	elseif c1 == "stopknock" then 
		Knock = nil
		TargetNoclipping = false 
		TargetReach = false 
		if plr.Character.Head:FindFirstChild("_targetvel_") then 
			plr.Character.Head:FindFirstChild("_targetvel_"):Destroy()
		end
		
	elseif c1 == "serverinfo" then 
		local players = #players:GetChildren()
		local ping = (plr:GetNetworkPing() * 1000 * 2)
		Notify(tostring(players).."\n"..tostring(ping), "SERVER INFO", 5)
	end
end

spawn(function()
repeat task.wait(.1) until plr.Character and plr.Character:FindFirstChild("FULLY_LOADED_CHAR")  
	plr.Character.BodyEffects.Movement.ChildAdded:Connect(function(c)
		if Antislow == true then 
			task.wait()
			c:Destroy()
		end
	end)
end)

spawn(function()
	pcall(function()
		while true and task.wait(.1) do 
			
			if Autodrop == true then 
				rst.MainEvent:FireServer("DropCash", "10000")
			end
			
			if Hidecash == true then 
				for i,v in next, ws.Ignored.Drop:GetChildren() do 
					if v.Name == "MoneyDrop" then
						v.Transparency = 1
						for i,v in pairs(v:GetDescendants()) do 
							if v:IsA("SurfaceGui") or v:IsA("BillboardGui") then 
								v.Enabled = false
							end
						end
						task.wait(.05)
					end
				end
			end
			
			
			if Cashesp == true then 
				for i,v in pairs(ws.Ignored.Drop:GetChildren()) do
					if v.Name == "MoneyDrop" then
						v.BillboardGui.AlwaysOnTop = true
						v.BillboardGui.Size = UDim2.new(0,25,0,25)
					end
				end
			end
			
			if Esp == true then 
				for i,v in pairs(players:GetChildren()) do 
					AcEsp()
				end
			end
			
			if Autostomp == true then 
				rst.MainEvent:FireServer("Stomp")
			end
			
			if Antigrab == true then 
				if plr and plr.Character then 
					if plr.Character:FindFirstChild("GRABBING_CONSTRAINT") then
						plr.Character:FindFirstChild("GRABBING_CONSTRAINT"):Destroy()
						task.wait(0.05)
						plr.Character:FindFirstChildWhichIsA("Humanoid").Sit = true
						task.wait(.1)
						plr.Character:FindFirstChildWhichIsA("Humanoid").Sit = false
					end
				end
			end
			
			if Cashaura == true then 
				for i,v in pairs(ws.Ignored.Drop:GetChildren()) do
					if v:IsA("Part") and v.Name == "MoneyDrop" then
						if (v.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 12 then
							fireclickdetector(v:FindFirstChildWhichIsA("ClickDetector"))
						end
					end
				end
			end
			
			if Reach == true or TargetReach == true then 
				local success, err = pcall(function()
					if plr.Character.BodyEffects.Attacking.Value == true then
						for i,v in pairs(players:GetChildren()) do
							if v and v.Character and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Humanoid").Health ~= 0 then 
								if (v.Character.HumanoidRootPart.Position - plr.Character.LeftHand.Position).Magnitude <= 50 then
									if plr.Character:FindFirstChildOfClass("Tool") then
										if plr.Character:FindFirstChildOfClass("Tool"):FindFirstChild('Handle') then
											firetouchinterest(plr.Character:FindFirstChildOfClass("Tool").Handle, v.Character.UpperTorso, 0)
										else
											firetouchinterest(plr.Character['RightHand'], v.Character.UpperTorso, 0)
											firetouchinterest(plr.Character['LeftHand'], v.Character.UpperTorso, 0)
											firetouchinterest(plr.Character['RightFoot'], v.Character.UpperTorso, 0)
											firetouchinterest(plr.Character['LeftFoot'], v.Character.UpperTorso, 0)
											firetouchinterest(plr.Character['RightLowerLeg'], v.Character.UpperTorso, 0)
											firetouchinterest(plr.Character['LeftLowerLeg'], v.Character.UpperTorso, 0)
										end
									end
								end
							end
						end
					end
				end)
			end
		end
	end)
end)


rs.RenderStepped:Connect(function()
	
	if Knock ~= nil then 
		if plr.Character and plr.Character:FindFirstChild("FULLY_LOADED_CHAR") and Knock and Knock.Character and Knock.Character:FindFirstChild("Humanoid") then 
			if not plr.Character.Head:FindFirstChild("_targetvel_") then 
				local vel = Instance.new("BodyVelocity")
				vel.Name = "_targetvel_"
				vel.MaxForce = Vector3.new(1e+12, 1e+12, 1e+12)
				vel.P = 1250
				vel.Parent = plr.Character.Head
			end
			local t = Knock.Character.HumanoidRootPart
			local tko = Knock.Character.BodyEffects["K.O"]
			if tko.Value == false then 
				plr.Character.HumanoidRootPart.CFrame = t.CFrame + Vector3.new(0, -7, 0)
				if plr.Backpack:FindFirstChild("Combat") then 
					plr.Backpack:FindFirstChild("Combat").Parent = plr.Character
				end
				plr.Character:FindFirstChild("Combat"):Activate()
			elseif tko.Value == true then 
				Knock = nil 
				if plr.Character.Head:FindFirstChild("_targetvel_") then 
					plr.Character.Head:FindFirstChild("_targetvel_"):Destroy()
				end
				TargetNoclipping = false 
				TargetReach = false 
				plr.Character.HumanoidRootPart.CFrame = CFrame.new(KnockOldPos)
			end
		end
	end
	
	if Targeting ~= nil then 
		if plr.Character and plr.Character:FindFirstChild("FULLY_LOADED_CHAR") and Targeting and Targeting.Character and Targeting.Character:FindFirstChild("Humanoid") then 
			if not plr.Character.Head:FindFirstChild("_vel_") then 
				local vel = Instance.new("BodyVelocity")
				vel.Name = "_vel_"
				vel.MaxForce = Vector3.new(1e+12, 1e+12, 1e+12)
				vel.P = 1250
				vel.Parent = plr.Character.Head
			end
			local t = Targeting.Character.HumanoidRootPart
			local tko = Targeting.Character.BodyEffects["K.O"]
			local tdead = Targeting.Character.BodyEffects["Dead"]
			if tko.Value == false and tdead.Value == false then 
				plr.Character.HumanoidRootPart.CFrame = t.CFrame + Vector3.new(0, -7, 0)
				if plr.Backpack:FindFirstChild("Combat") then 
					plr.Backpack:FindFirstChild("Combat").Parent = plr.Character
				end
				plr.Character:FindFirstChild("Combat"):Activate()
			elseif tko.Value == true and tdead.Value == false then 
				plr.Character.HumanoidRootPart.CFrame = CFrame.new(Targeting.Character.UpperTorso.Position) + Vector3.new(0,2,0)
				rst.MainEvent:FireServer("Stomp")
			elseif tko.Value == true and tdead.Value == true then
				plr.Character.HumanoidRootPart.CFrame = t.CFrame + Vector3.new(0, -10, 0)
			end
		end
	end
end)

rs.Stepped:Connect(function()
	if plr.Character:FindFirstChildWhichIsA("Humanoid") then 
		
		if Antistomp == true and plr.Character:FindFirstChild("BodyEffects") then 
			if plr.Character.BodyEffects["K.O"].Value == true then 
				plr.Character.Humanoid.Health = 0
				for i,v in pairs(plr.Character:GetChildren()) do 
					if v:IsA("BasePart") or v:IsA("Accessory") then 
						v:Destroy()
					end
				end
			end
		end
		
		if Noclipping == true or TargetNoclipping == true then 
				for i,v in pairs(plr.Character:GetDescendants()) do 
					if v:IsA("BasePart") then 
						v.CanCollide = false
					end
				end
			end
	end
end)

plr.CharacterAdded:Connect(function(c)
	repeat task.wait() until c and c:FindFirstChild("Humanoid") and c:FindFirstChild("Head")
	
	if Hideuser == true then 
		local oldpos = plr.Character.HumanoidRootPart.Position
		repeat task.wait() until c:FindFirstChild("FULLY_LOADED_CHAR")
		task.wait(.1)
		local mask = ws.Ignored.Shop["[Surgeon Mask] - $25"].Head
		repeat
			task.wait() 
			plr.Character.HumanoidRootPart.CFrame = mask.CFrame + Vector3.new(math.random(-1,1), 3, math.random(-1,1))
			fireclickdetector(mask.Parent.ClickDetector)
		until plr.Backpack:FindFirstChild("Mask")
		task.wait()
		local item = plr.Backpack:FindFirstChild("Mask")
		item.Parent = plr.Character
		item:Activate()
		repeat task.wait() until plr.Character:FindFirstChild("In-gameMask") and plr.Character:FindFirstChild("In-gameMask").Handle
		plr.Character:FindFirstChild("In-gameMask").Handle:Destroy()
		plr.Character.HumanoidRootPart.CFrame = CFrame.new(oldpos)
	end
	if AutoFly_1 == true and AutoFly_2 == false then 
		task.wait() 
		Fly_1()
	end
	
	if AutoFly_2 == true and AutoFly_1 == false then 
		task.wait() 
		Fly_2()
	end
end)

Cmdbar.FocusLost:Connect(function()
	local str = Cmdbar.Text:lower():gsub("^%s", ""):split(" ")

	for i,v in pairs(RawCommands) do 
		if str[1] == v then 
			Run(str[1], str[2])
		end
	end
	task.wait()
	Close()
end)


game:GetService("UserInputService").InputBegan:Connect(function(i, p)
	if not p then 
		if i.KeyCode == Enum.KeyCode.Semicolon then 
			Open()
		end
		
		if Qtp == true then 
			if i.KeyCode == Enum.KeyCode.Q then 
				plr.Character.HumanoidRootPart.CFrame = plr:GetMouse().Hit * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0)) + Vector3.new(0,5,0)
				task.wait(0.01)
					plr.Character:FindFirstChildWhichIsA("Humanoid"):ChangeState(Enum.HumanoidStateType.GettingUp)
					
				
			end
		end
	end
end)


spawn(function()
	local CommandsLoaded = 0
	for i,v in next, Commands do 
		local NewCmd = Cmd:Clone()
		NewCmd.Visible = true 
		NewCmd.Name = v:split(" |")[1]:gsub("<user>", ""):gsub("<full user>", ""):gsub("<display>", ""):gsub("<id>", ""):gsub("<am>", ""):gsub("<item>", "")
		NewCmd.Parent = Holder
		NewCmd.Text = ("["..tostring(i).."] "..v:lower())
		if NewCmd.TextBounds.X >= 188 then 
			NewCmd.Size = UDim2.new(0, 188, 0, 28)
			NewCmd.TextWrapped = true
		else 
			NewCmd.Size = UDim2.new(0, 188, 0, 20)
		end
		
		CommandsLoaded = CommandsLoaded + 1
		task.wait()
	end
	print(CommandsLoaded.." Commands loaded!")
	Cmd:Destroy()
end)


Cmdbar.Changed:Connect(function(prop)
	if prop == "Text" then 
		if Cmdbar.Text ~= "" then
			if Cmdbar.Text ~= " " or Cmdbar.Text ~= ";" then 
				for i,v in pairs(Holder:GetChildren()) do 
					if v:IsA("TextLabel") then 
						v.Visible = false
						if v.Text:gsub("["..i.."]", ""):split(" | ")[1]:gsub("<user>", ""):gsub("<full user>", ""):gsub("<display>", ""):gsub("<id>", ""):gsub("<am>", ""):gsub("<item>", ""):find(Cmdbar.Text) then 
							v.Visible = true
						end
					end
				 end
			end
		else 
			for i,v in pairs(Holder:GetChildren()) do 
				if v:IsA("TextLabel") then 
					v.Visible = true
				end
			end
		end
	end
end)


