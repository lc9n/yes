		game:GetService("TweenService"):Create(UIGradient, TweenInfo.new(2, Enum.EasingStyle.Quad), { Offset = Vector2.new(0, UIGradient.Offset.Y - 1)} ):Play()
		game:GetService("TweenService"):Create(UIGradient, TweenInfo.new(2, Enum.EasingStyle.Quad), { Rotation = UIGradient.Rotation + 20 } ):Play()
		task.wait(2.1)
	end
end)

