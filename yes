if not plr.Character:FindFirstChild("FULLY_LOADED_CHAR") then 
	repeat task.wait() until plr.Character:FindFirstChild("FULLY_LOADED_CHAR")
end

Notify("Successfully executed, press [ ; ] to open.", "IKU COMMANDS", 8)


function Run(cmd1, cmd2)
	
 	spawn(function()
		Close()
	end)
	
	local c1, c2 = cmd1, nil
	
	if cmd2 then 
		c2 = cmd2
	end
	
	if c1 == "fly" then 
		if plr.Character.Head:FindFirstChildWhichIsA("BodyGyro") then 
			plr.Character.Head:FindFirstChildWhichIsA("BodyGyro"):Destroy()
		end
		if plr.Character.Head:FindFirstChildWhichIsA("BodyVelocity") then 
			plr.Character.Head:FindFirstChildWhichIsA("BodyVelocity"):Destroy()
		end
		AutoFly_1 = not AutoFly_1
		AutoFly_2 = false 
		Flying_2 = false 
		Flying_1 = true
		if AutoFly_1 == true then 
			Fly_1()
		end
		
	elseif c1 == "fly2" then 
		if workspace:FindFirstChild("_nigger_") then 
			workspace._nigger_:Destroy()
		end
		AutoFly_2 = not AutoFly_2
		AutoFly_1 = false 
		Flying_1 = false
		Flying_2 = true
		if AutoFly_2 == true then 
			Fly_2()
		end
		
	elseif c1 == "flyspeed" then 
		FlySpeed = tonumber(c2)
		print(FlySpeed)
		
	elseif c1 == "noclip" then 
		Noclipping = true 
		
	elseif c1 == "clip" then
		Noclipping = false
		
	elseif c1 == "reach" then 
		Reach = true 
		
	elseif c1 == "unreach" then 
		Reach = false 
		
	elseif c1 == "to" then 
		local t = GetPlayer(c2)
		task.wait()
		plr.Character.HumanoidRootPart.CFrame = t.Character.HumanoidRootPart.CFrame +  t.Character.HumanoidRootPart.CFrame.LookVector * Vector3.new(3,0,3)
		
	elseif c1 == "view" then 
		local t = GetPlayer(c2)
		task.wait()
		ws:FindFirstChildWhichIsA("Camera").CameraSubject = t.Character:FindFirstChildWhichIsA("Humanoid")
		
	elseif c1 == "unview" then 
		ws:FindFirstChildWhichIsA("Camera").CameraSubject = plr.Character:FindFirstChildWhichIsA("Humanoid")

	elseif c1 == "fling" then 
		local t = GetPlayer(c2)
		OldFlingPos = plr.Character.HumanoidRootPart.Position
		task.wait()
		Flinging = t
		
		if Flinging ~= nil then 
			local Loop
			local loopFunction = function(FlingEnemy)
				FlingTorso = FlingEnemy.Character:FindFirstChild("HumanoidRootPart")
				local dis = 3.85
				local increase = 6
				if FlingEnemy.Character.Humanoid.MoveDirection.X < 0 then
					xchange = math.random(-7,-4)
				elseif FlingEnemy.Character.Humanoid.MoveDirection.X > 0  then
					xchange = math.random(4,7)
				else
					xchange = 0
				end
				if FlingEnemy.Character.Humanoid.MoveDirection.Z < 0 then
					zchange = math.random(-7,-4)
				elseif FlingEnemy.Character.Humanoid.MoveDirection.Z > 0 then
					zchange = math.random(4,7)
				else
					zchange = 0
				end          
				if plr.Character and FlingTorso and FlingEnemy and Flinging.Character:FindFirstChildWhichIsA("Humanoid") then
					plr.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(11)
					plr.Character.HumanoidRootPart.CFrame = CFrame.new(FlingTorso.Position.X + math.random(-dis,dis) + xchange, FlingTorso.Position.Y, FlingTorso.Position.Z + math.random(-dis,dis) + zchange) * CFrame.Angles(math.rad(plr.Character.HumanoidRootPart.Orientation.X + 350),math.rad(plr.Character.HumanoidRootPart.Orientation.Y + 200),math.rad(plr.Character.HumanoidRootPart.Orientation.Z + 240))
					plr.Character.HumanoidRootPart.Velocity = Vector3.new(500000,500000,500000)
				end
				wait(0.05)
			end
			local Start = function(FlingEnemy)
				Loop = rs.Heartbeat:Connect(function()
					loopFunction(FlingEnemy)
				end)
			end
			local Pause = function()
				Loop:Disconnect()
			end
			Start(Flinging)
			repeat task.wait() until 
			Flinging == nil 
			Pause()
		end
	
	elseif c1 == "chatspy" then
		chatspy()
		
	elseif c1 == "unfling" then 
		Flinging = nil
		task.wait()
		for i = 1,250 do
			plr.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(2)
			plr.Character.PrimaryPart.Velocity = Vector3.new(0,0,0)
			plr.Character.PrimaryPart.RotVelocity = Vector3.new(0,0,0)
			plr.Character:FindFirstChild("HumanoidRootPart").CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
			task.wait(0.00001)
		end
		
	elseif c1 == "bag" then 
		local oldpos = plr.Character.HumanoidRootPart.Position
		local t = GetPlayer(c2)
		local bag  =ws.Ignored.Shop["[BrownBag] - $25"].Head
		task.wait() 
		
		repeat 
			task.wait() 
			plr.Character.HumanoidRootPart.CFrame = bag.CFrame + Vector3.new(math.random(-1,1), 3, math.random(-1,1))
			fireclickdetector(bag.Parent.ClickDetector)
		until plr.Backpack:FindFirstChild("[BrownBag]")
		task.wait()
		local tool = plr.Backpack:FindFirstChild("[BrownBag]")
		repeat 
			if t.Character and t.Character:FindFirstChild("FULLY_LOADED_CHAR") then 	
				task.wait() 
				tool.Parent = plr.Character
				tool:Activate() 
				tool:Deactivate() 
				
				for i,v in pairs(t.Character:GetChildren()) do 
					if v:IsA("BasePart") then 
						firetouchinterest(tool.Handle, v, 0)
						firetouchinterest(tool.Handle, v, 1)
					end
				end
			end
		until tool.Parent == nil or not tool
		task.wait(.1)
		plr.Character.HumanoidRootPart.CFrame = CFrame.new(oldpos)
		
	elseif c1 == "unbag" then 
		for i,v in pairs(plr.Character:GetChildren()) do 
			if v.Name == "Christmas_Sock" then 
				v:Destroy()
			end
		end

	elseif c1 == "antibag" then
		local LocalPlayer = game:GetService("Players").LocalPlayer
		local char = LocalPlayer.Character
		char.ChildAdded:Connect(function(sock)
			if sock:IsA("MeshPart") then do
					wait(0)
					sock:Destroy()
				end
			end
		end)


	elseif c1 == "target" then 
		OldTargetPos = plr.Character.HumanoidRootPart.Position
		Targeting = GetPlayer(c2)
		Knock = nil
		TargetNoclipping = true 
		TargetReach = true 

		
		
