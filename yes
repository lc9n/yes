if not game:IsLoaded() then 
	repeat task.wait() until game:IsLoaded() and game:GetService("Players").LocalPlayer and game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid")
	task.wait(2.5)
end

if game:GetService("CoreGui"):FindFirstChild("SG") then
	return
end

